FROM crime_scene_report
WHERE date = 20180115 AND city = &quot;SQL City&quot; AND type = &quot;murder&quot;;


--Annabel Miller's clue
SELECT 
    person.name,
    person.address_street_name,
    person.address_number,
    interview.transcript,
    get_fit_now_check_in.check_in_date,
    get_fit_now_check_in.check_in_time,
    get_fit_now_check_in.check_out_time
FROM 
    person
JOIN 
    interview
ON 
    person.id = interview.person_id
JOIN 
    get_fit_now_member
ON 
    person.id = get_fit_now_member.person_id
JOIN 
    get_fit_now_check_in
ON 
    get_fit_now_member.id = get_fit_now_check_in.membership_id
WHERE 
    person.address_street_name = &quot;Franklin Ave&quot; 
    AND person.name LIKE &quot;%Annabel%&quot;;
	
	
--Morty Schapiro's clue
SELECT id AS person_id, name, transcript
FROM 
	person
JOIN 
	interview
ON 
	person.id = interview.person_id
WHERE 
	address_street_name = &quot;Northwestern Dr&quot;
ORDER BY 
	address_number DESC
LIMIT 1;


--Clues By Morty's transcript
SELECT person.name, get_fit_now_check_in.membership_id, check_in_date, check_in_time, check_out_time, drivers_license.id AS drivers_license_id, plate_number
FROM 
	get_fit_now_check_in
JOIN 
	get_fit_now_member
ON 
	get_fit_now_check_in.membership_id = get_fit_now_member.id
JOIN 
	person
ON 
	get_fit_now_member.person_id = person.id
JOIN 
	drivers_license
ON 
	drivers_license.id = person.license_id
WHERE 
	membership_id LIKE &quot;48Z%&quot;
	AND plate_number LIKE &quot;%H42W%&quot;;










