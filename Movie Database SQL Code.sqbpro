"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">--CREATING TABLES

CREATE TABLE Company_Table (
	company_id INT UNIQUE PRIMARY KEY,
	average_score REAL,
	total_movies INT
);


CREATE TABLE Movies_Table (
	movie_id INT UNIQUE PRIMARY KEY,
	company_id INT,  
	year INT,
	movie_score REAL,
	genre TEXT, 
	runtime INT,
	rating TEXT,
	FOREIGN KEY (company_id) REFERENCES Company_Table(company_id)
);


CREATE TABLE Profit_Table (
    movie_profit REAL PRIMARY KEY,
    movie_id INT UNIQUE,
    budget REAL,
    gross REAL,
    genre TEXT,  
    FOREIGN KEY (movie_id) REFERENCES Movies_Table(movie_id),
    FOREIGN KEY (genre) REFERENCES Movie_Genre(genre)
);


CREATE TABLE Movie_Actors (
    actor_id INTEGER PRIMARY KEY AUTOINCREMENT,
    actor_name TEXT,
    movie_id INTEGER,
    FOREIGN KEY (movie_id) REFERENCES Movies_Table(movie_id)
);


CREATE TABLE Movie_Genre (
    genre TEXT PRIMARY KEY,
    genre_movie_count INTEGER
);



-- INSERTING DATE INTO TABLES

INSERT INTO Company_Table (company_id, total_movies, average_score)
SELECT DISTINCT company AS company_id,
       COUNT(name) AS total_movies,
       AVG(score) AS average_score
FROM Movie_Dataset
GROUP BY company;

--Total Amount of Movies Per Company in Descending Order
SELECT company_id, total_movies, average_score
FROM Company_Table
ORDER BY total_movies DESC


INSERT INTO Movies_Table (movie_id, company_id, year, movie_score, genre, runtime, rating)
SELECT ROW_NUMBER() OVER () AS movie_id, 
       company AS company_id,
       year,
       score AS movie_score,
       genre,
       runtime,
       rating
FROM Movie_Dataset;


INSERT INTO Profit_Table (movie_profit, movie_id, budget, gross)
SELECT (gross - budget) AS movie_profit, 
       ROW_NUMBER() OVER () AS movie_id,
       budget, 
       gross
FROM Movie_Dataset;

UPDATE Profit_Table
SET genre = (
    SELECT Movie_Dataset.genre
    FROM Movie_Dataset
    JOIN Movies_Table ON Movie_Dataset.year = Movies_Table.year
    WHERE Movies_Table.movie_id = Profit_Table.movie_id
);


INSERT INTO Movie_Actors (actor_name, movie_id)
SELECT Movie_Dataset.star AS actor_name, Movies_Table.movie_id
FROM Movie_Dataset
JOIN Movies_Table ON Movie_Dataset.company = Movies_Table.company_id;


INSERT INTO Movie_Genre (genre, genre_movie_count)
SELECT 
    genre AS genre,
    COUNT(genre) AS genre_movie_count
FROM 
    Movie_Dataset
GROUP BY 
    genre;

--Updating Tables
ALTER TABLE Movie_Genre ADD COLUMN profit_per_genre
	
